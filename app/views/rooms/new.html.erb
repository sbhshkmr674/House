<div class="building-form">
          <%= form_for [@floor, @floor.rooms.new] do |f| %>
         
          <div class="form-group">
             <div class="col-lg-3">
                <%= f.label :image %>
             </div>
             <div class="col-lg-9">
                <%= f.file_field :image %>
             </div>
          </div>
          <div class="form-group text-center">
      <%= f.button :submit, class: 'btn btn-success' %>
    </div>
    <% end %>
</div>